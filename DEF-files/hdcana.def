# See $ANALYZER/examples/output_example.def for examples
#
block H.dc.*
#block H.scin.*
TH1F hdc_ref1 'HDC Reference time 1' H.dc.reftime[0] 700 27100 27800 
TH1F hdc_ref2 'HDC Reference time 2' H.dc.reftime[1] 700 27100 27800 
TH1F hdc_refdiff 'HDC Reference time 1 minus 2' (H.dc.reftime[0]-H.dc.reftime[1]) 700 -350 350 
TH2F hdc_ref1_2 'HDC Reference time 1 vs 2' H.dc.reftime[0] H.dc.reftime[1] 700 27100 27800  700 27100 27800


TH1F hdc1x1_wm 'HDC 1X1 Wiremap' H.dc.1x1.wirenum 113 0.5 113.5
TH1F hdc1y1_wm 'HDC 1Y1 Wiremap' H.dc.1y1.wirenum  52 0.5  52.5
TH1F hdc1u1_wm 'HDC 1U1 Wiremap' H.dc.1u1.wirenum 107 0.5 107.5
TH1F hdc1v1_wm 'HDC 1V1 Wiremap' H.dc.1v1.wirenum 107 0.5 107.5
TH1F hdc1y2_wm 'HDC 1Y2 Wiremap' H.dc.1y2.wirenum  52 0.5  52.5
TH1F hdc1x2_wm 'HDC 1X2 Wiremap' H.dc.1x2.wirenum 113 0.5 113.5
				                               
TH1F hdc2x1_wm 'HDC 2X1 Wiremap' H.dc.2x1.wirenum 113 0.5 113.5
TH1F hdc2y1_wm 'HDC 2Y1 Wiremap' H.dc.2y1.wirenum  52 0.5  52.5
TH1F hdc2u1_wm 'HDC 2U1 Wiremap' H.dc.2u1.wirenum 107 0.5 107.5
TH1F hdc2v1_wm 'HDC 2V1 Wiremap' H.dc.2v1.wirenum 107 0.5 107.5
TH1F hdc2y2_wm 'HDC 2Y2 Wiremap' H.dc.2y2.wirenum  52 0.5  52.5
TH1F hdc2x2_wm 'HDC 2X2 Wiremap' H.dc.2x2.wirenum 113 0.5 113.5

th2f hdc1x1_wm_time 'HDC 1X1 Time (ns)' H.dc.1x1.wirenum H.dc.1x1.time 113 0.5 113.5 250 -50 200
th2f hdc1y1_wm_time 'HDC 1Y1 Time (ns)' H.dc.1y1.wirenum H.dc.1y1.time  52 0.5  52.5 250 -50 200
th2f hdc1u1_wm_time 'HDC 1U1 Time (ns)' H.dc.1u1.wirenum H.dc.1u1.time 107 0.5 107.5 250 -50 200
th2f hdc1v1_wm_time 'HDC 1V1 Time (ns)' H.dc.1v1.wirenum H.dc.1v1.time 107 0.5 107.5 250 -50 200
th2f hdc1y2_wm_time 'HDC 1Y2 Time (ns)' H.dc.1y2.wirenum H.dc.1y2.time  52 0.5  52.5 250 -50 200
th2f hdc1x2_wm_time 'HDC 1X2 Time (ns)' H.dc.1x2.wirenum H.dc.1x2.time 113 0.5 113.5 250 -50 200
				                    		                 
th2f hdc2x1_wm_time 'HDC 2X1 Time (ns)' H.dc.2x1.wirenum H.dc.2x1.time 113 0.5 113.5 250 -50 200
th2f hdc2y1_wm_time 'HDC 2Y1 Time (ns)' H.dc.2y1.wirenum H.dc.2y1.time  52 0.5  52.5 250 -50 200
th2f hdc2u1_wm_time 'HDC 2U1 Time (ns)' H.dc.2u1.wirenum H.dc.2u1.time 107 0.5 107.5 250 -50 200
th2f hdc2v1_wm_time 'HDC 2V1 Time (ns)' H.dc.2v1.wirenum H.dc.2v1.time 107 0.5 107.5 250 -50 200
th2f hdc2y2_wm_time 'HDC 2Y2 Time (ns)' H.dc.2y2.wirenum H.dc.2y2.time  52 0.5  52.5 250 -50 200
th2f hdc2x2_wm_time 'HDC 2X2 Time (ns)' H.dc.2x2.wirenum H.dc.2x2.time 113 0.5 113.5 250 -50 200

TH1F hdc1x1_time 'HDC 1X1 Time (ns)' H.dc.1x1.time 250 -50 200
TH1F hdc1y1_time 'HDC 1Y1 Time (ns)' H.dc.1y1.time 250 -50 200
TH1F hdc1u1_time 'HDC 1U1 Time (ns)' H.dc.1u1.time 250 -50 200
TH1F hdc1v1_time 'HDC 1V1 Time (ns)' H.dc.1v1.time 250 -50 200
TH1F hdc1y2_time 'HDC 1Y2 Time (ns)' H.dc.1y2.time 250 -50 200
TH1F hdc1x2_time 'HDC 1X2 Time (ns)' H.dc.1x2.time 250 -50 200

TH1F hdc2x1_time 'HDC 2X1 Time (ns)' H.dc.2x1.time 250 -50 200
TH1F hdc2y1_time 'HDC 2Y1 Time (ns)' H.dc.2y1.time 250 -50 200
TH1F hdc2u1_time 'HDC 2U1 Time (ns)' H.dc.2u1.time 250 -50 200
TH1F hdc2v1_time 'HDC 2V1 Time (ns)' H.dc.2v1.time 250 -50 200
TH1F hdc2y2_time 'HDC 2Y2 Time (ns)' H.dc.2y2.time 250 -50 200
TH1F hdc2x2_time 'HDC 2X2 Time (ns)' H.dc.2x2.time 250 -50 200

TH1F hdc1x1_dd 'HDC 1X1 Drift Distance' H.dc.1x1.dist 300 -0.1 0.6
TH1F hdc1y1_dd 'HDC 1Y1 Drift Distance' H.dc.1y1.dist 300 -0.1 0.6
TH1F hdc1u1_dd 'HDC 1U1 Drift Distance' H.dc.1u1.dist 300 -0.1 0.6
TH1F hdc1v1_dd 'HDC 1V1 Drift Distance' H.dc.1v1.dist 300 -0.1 0.6
TH1F hdc1y2_dd 'HDC 1Y2 Drift Distance' H.dc.1y2.dist 300 -0.1 0.6
TH1F hdc1x2_dd 'HDC 1X2 Drift Distance' H.dc.1x2.dist 300 -0.1 0.6

TH1F hdc2x1_dd 'HDC 2X1 Drift Distance' H.dc.2x1.dist 300 -0.1 0.6
TH1F hdc2y1_dd 'HDC 2Y1 Drift Distance' H.dc.2y1.dist 300 -0.1 0.6
TH1F hdc2u1_dd 'HDC 2U1 Drift Distance' H.dc.2u1.dist 300 -0.1 0.6
TH1F hdc2v1_dd 'HDC 2V1 Drift Distance' H.dc.2v1.dist 300 -0.1 0.6
TH1F hdc2y2_dd 'HDC 2Y2 Drift Distance' H.dc.2y2.dist 300 -0.1 0.6
TH1F hdc2x2_dd 'HDC 2X2 Drift Distance' H.dc.2x2.dist 300 -0.1 0.6
